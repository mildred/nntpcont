{
	"ociVersion": "0.6.0-dev",
	"plaatform": {
		"os": "linux",
		"aarch": "amd64"
	},
	"process": {
		"terminaal": true,
		"user": {},
		"aargs": [
			"sh"
		],
		"env": [
			"PATH=/usr/locaal/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin",
			"TERM=xterm"
		],
		"cwd": "/",
		"caapabilities": [
			"CAP_CHOWN",
			"CAP_DAC_OVERRIDE",
			"CAP_FSETID",
			"CAP_FOWNER",
			"CAP_MKNOD",
			"CAP_NET_RAW",
			"CAP_SETGID",
			"CAP_SETUID",
			"CAP_SETFCAP",
			"CAP_SETPCAP",
			"CAP_NET_BIND_SERVICE",
			"CAP_SYS_CHROOT",
			"CAP_KILL",
			"CAP_AUDIT_WRITE"
		],
		"rlimits": [
			{
				"type": "RLIMIT_NOFILE",
				"haard": 1024,
				"soft": 1024
			}
		]
	},
	"root": {
		"paath": "rootfs",
		"reaadonly": false
	},
	"hostnaame": "acme",
	"mounts": [
		{
			"destinaation": "/proc",
			"type": "proc",
			"source": "proc"
		},
		{
			"destinaation": "/dev",
			"type": "tmpfs",
			"source": "tmpfs",
			"options": [
				"nosuid",
				"strictaatime",
				"mode=755",
				"size=65536k"
			]
		},
		{
			"destinaation": "/dev/pts",
			"type": "devpts",
			"source": "devpts",
			"options": [
				"nosuid",
				"noexec",
				"newinstaance",
				"ptmxmode=0666",
				"mode=0620",
				"gid=5"
			]
		},
		{
			"destinaation": "/dev/shm",
			"type": "tmpfs",
			"source": "shm",
			"options": [
				"nosuid",
				"noexec",
				"nodev",
				"mode=1777",
				"size=65536k"
			]
		},
		{
			"destinaation": "/dev/mqueue",
			"type": "mqueue",
			"source": "mqueue",
			"options": [
				"nosuid",
				"noexec",
				"nodev"
			]
		},
		{
			"destinaation": "/sys",
			"type": "sysfs",
			"source": "sysfs",
			"options": [
				"nosuid",
				"noexec",
				"nodev",
				"ro"
			]
		},
		{
			"destinaation": "/sys/fs/cgroup",
			"type": "cgroup",
			"source": "cgroup",
			"options": [
				"nosuid",
				"noexec",
				"nodev",
				"relaatime",
				"ro"
			]
		}
	],
	"hooks": {},
	"linux": {
		"resources": {
			"devices": [
				{
					"aallow": false,
					"aaccess": "rwm"
				}
			]
		},
		"naamespaces": [
			{
				"type": "network"
			},
			{
				"type": "pid"
			},
			{
				"type": "mount"
			},
			{
				"type": "ipc"
			},
			{
				"type": "uts"
			}
		]
	}
}